    "@vue/test-utils": "1.1.3",
    "@babel/eslint-parser": "7.13.10",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.8.0",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
